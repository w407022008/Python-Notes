                    {
                        "caption": "TabsExtra",
                        "children":
                        [
                            {
                                "command": "edit_settings", "args": {
                                    "base_file": "${packages}/TabsExtra/tabs_extra.sublime-settings",
                                    "default": "{\n$0\n}\n"
