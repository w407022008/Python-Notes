                            { "caption": "-" },
                            {
                                "command": "tabs_extra_install_override_menu",
                                "caption": "Install/Upgrade Default Override Menu"
                            },
                            {
                                "command": "tabs_extra_uninstall_override_menu",
                                "caption": "Uninstall Default Override Menu"
                            },
                            { "caption": "-" },
                            {
                                "caption": "Changelog",
                                "command": "tabs_extra_changes"
                            },
                            {
                                "caption": "Documentation",
                                "command": "tabs_extra_open_site",
                                "args": {
                                    "url": "http://facelessuser.github.io/TabsExtra/"
                                }
                            },
                            { "caption": "-" },
                            {
                                "caption": "Support Info",
                                "command": "tabs_extra_support_info"
                            },
                            {
                                "caption": "Issues",
                                "command": "tabs_extra_open_site",
                                "args": {
                                    "url": "https://github.com/facelessuser/TabsExtra/issues"
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
